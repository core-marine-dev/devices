{
  "name": "@coremarine/nmea-parser",
  "version": "1.6.5",
  "description": "Library to parse NMEA 0183 sentences",
  "author": "CoreMarine",
  "license": "ISC",
  "homepage": "https://github.com/core-marine-dev/devices/packages/nmea-parser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/core-marine-dev/devices.git"
  },
  "bugs": {
    "url": "https://github.com/core-marine-dev/devices/issues"
  },
  "keywords": [
    "nmea",
    "nmea0183",
    "0183",
    "gps",
    "gnss"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && cp -r ./src/protocols ./lib",
    "lint": "ts-standard",
    "format": "ts-standard --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "js-yaml": "4.1.0",
    "zod": "3.23.6",
    "zod-to-json-schema": "3.23.0"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.9"
  },
  "ts-standard": {
    "ignore": [
      "tests",
      "vitest.config.ts",
      "tsup.config.ts"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  }
}
