{
  "name": "@coremarine/sbf-parser",
  "version": "1.0.2",
  "author": "CoreMarine",
  "description": "It is a library to parse SBF data. SBF is a private binary protocol of Septentrio trademark which uses in its GNSS devices",
  "license": "MIT",
  "homepage": "https://github.com/core-marine-dev/devices/packages/septentrio-sbf#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/core-marine-dev/devices.git"
  },
  "bugs": {
    "url": "https://github.com/core-marine-dev/devices/issues"
  },
  "keywords": [
    "coremarine",
    "core-marine",
    "septentrio",
    "sbf",
    "gnss",
    "gps",
    "binary",
    "protocol"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "types": "mkdir node_modules/@types/gpstime && cp src/gpstime.d.ts node_modules/@types/gpstime/index.d.ts",
    "build": "tsc",
    "start": "ts-node-dev ./src/index.ts"
  },
  "dependencies": {
    "crc": "^4.3.2",
    "gpstime": "^1.0.3"
  }
}
